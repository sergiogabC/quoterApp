<div>
  <table class="table-auto border-collapse w-full min-w-max" id="tableRows">
    <thead>
      <tr>
        <th class="cardInputHead"><strong>Tipo</strong></th>
        <th class="cardInputHead"><strong>Categoría</strong></th>
        <th class="cardInputHead"><strong>Subcategoría</strong></th>
        <th class="cardInputHead"><strong>Pieza del Fabricante</strong></th>
        <th class="cardInputHead"><strong>Codigo Producto</strong></th>
        <th class="cardInputHead"><strong>Descripción</strong></th>
        <th class="cardInputHead"><strong>Cantidad</strong></th>
        <th class="cardInputHead"><strong>Unidad de Medida</strong></th>
        <th class="cardInputHead"><strong>Descuento</strong></th>
        <th class="cardInputHead"><strong>Financiamiento</strong></th>
        <th class="cardInputHead"><strong>Dueño</strong></th>
      </tr>
    </thead>
    <tbody id="tbodyRows">
      <tr>
        <td class="cardInputData">
          <input required type="text" name="Tipo" id="Tipo" />
        </td>
        <td class="cardInputData">
          <input required type="text" name="Categoria" id="Categoria" />
        </td>
        <td class="cardInputData">
          <input required type="text" name="Subcategoria" id="Subcategoria" />
        </td>
        <td class="cardInputData">
          <input required type="text" name="PiezaFabricante" id="PiezaFabricante" />
        </td>
        <td class="cardInputData">
          <input required type="text" name="CodigoProducto" id="CodigoProducto" />
        </td>
        <td class="cardInputData">
          <input required type="text" name="Descripcion" id="Descripcion" />
        </td>
        <td class="cardInputData">
          <input required type="number" class="inputNumber" name="Cantidad" id="Cantidad" />
        </td>
        <td class="cardInputData">
          <input required type="text" name="UnidadMedida" id="UnidadMedida" />
        </td>
        <td class="cardInputData">
          <input required type="number" class="inputNumber" name="Descuento" id="Descuento" />
        </td>
        <td class="cardInputData">
          <input required type="text" name="Financiamiento" id="Financiamiento" />
        </td>
        <td class="cardInputData">
          <input required type="text" name="Dueño" id="Dueño" />
        </td>
        <td class="tdBoton" id="tdBoton">          
          <div class="buttonMenos">
            <button type="button" class="tdMenos" form="">-</button>
          </div>
        </td>
      </tr>
    </tbody>    
  </table>
  <div>
    <div class="buttonMas">
      <button type="button" id="tdMas">+</button>
    </div>
  </div>
</div>


